    <div class="notification" id="notification">Operation completed successfully!</div>

    <script>
        // Funcionalidad de pestañas
        document.querySelectorAll('.tab').forEach(tab => {
            tab.addEventListener('click', () => {
                document.querySelectorAll('.tab').forEach(t => t.classList.remove('active'));
                document.querySelectorAll('.tab-content').forEach(c => c.classList.remove('active'));
                
                tab.classList.add('active');
                document.getElementById(tab.dataset.tab).classList.add('active');
            });
        });
        
        // Generador de keys
        document.getElementById('generate-keys').addEventListener('click', () => {
            const prefix = document.getElementById('key-prefix').value;
            const length = parseInt(document.getElementById('key-length').value);
            const count = parseInt(document.getElementById('key-count').value);
            
            let keys = [];
            for (let i = 0; i < count; i++) {
                let key = prefix;
                const chars = 'ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789';
                
                for (let j = 0; j < length - prefix.length; j++) {
                    key += chars.charAt(Math.floor(Math.random() * chars.length));
                }
                
                keys.push(key);
            }
            
            document.getElementById('generated-key').value = keys.join('\n');
            showNotification('Keys generated successfully!');
        });
        
        // Copiar key al portapapeles
        document.getElementById('copy-key').addEventListener('click', () => {
            const keyField = document.getElementById('generated-key');
            keyField.select();
            document.execCommand('copy');
            showNotification('Key copied to clipboard!');
        });
        
        // Búsqueda en tablas
        document.getElementById('key-search').addEventListener('input', (e) => {
            const searchTerm = e.target.value.toLowerCase();
            const rows = document.querySelectorAll('#keys-list tr');
            
            rows.forEach(row => {
                const key = row.cells[0].textContent.toLowerCase();
                row.style.display = key.includes(searchTerm) ? '' : 'none';
            });
        });
        
        document.getElementById('user-search').addEventListener('input', (e) => {
            const searchTerm = e.target.value.toLowerCase();
            const rows = document.querySelectorAll('#users-list tr');
            
            rows.forEach(row => {
                const user = row.cells[0].textContent.toLowerCase();
                row.style.display = user.includes(searchTerm) ? '' : 'none';
            });
        });
        
        // Simular datos actualizados
        setInterval(() => {
            document.getElementById('active-users-count').textContent = 
                Math.floor(Math.random() * 5) + 15;
        }, 10000);
        
        // Funciones de ban/desban
        function banKey(key) {
            showNotification(`Key ${key} has been banned!`);
            // Aquí iría la lógica real para banear la key
        }
        
        function unbanKey(key) {
            showNotification(`Key ${key} has been unbanned!`);
            // Aquí iría la lógica real para desbanear la key
        }
        
        // Sistema de notificaciones
        function showNotification(message) {
            const notification = document.getElementById('notification');
            notification.textContent = message;
            notification.classList.add('show');
            
            setTimeout(() => {
                notification.classList.remove('show');
            }, 3000);
        }
    </script>
</body>
